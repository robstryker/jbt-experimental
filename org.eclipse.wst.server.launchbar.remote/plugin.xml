<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.remote.core.remoteServices">
      <connectionType
            canAdd="false"
            id="org.eclipse.wst.server.launchbar.remote.connection"
            name="Server Adapter"
            scheme="dummyscheme">
      </connectionType>
      <connectionTypeService
            connectionTypeId="org.eclipse.wst.server.launchbar.remote.connection"
            factory="org.eclipse.wst.server.launchbar.remote.ui.RemoteTypeUIService$Factory"
            service="org.eclipse.remote.ui.IRemoteUIConnectionService">
      </connectionTypeService>
   </extension>
   
   
   
   <extension
         point="org.eclipse.launchbar.core.launchBarContributions">
      <targetType
            connectionTypeId="org.eclipse.wst.server.launchbar.remote.connection"
            id="org.eclipse.wst.server.launchbar.remote.connection">
      </targetType>
      <objectProvider
            class="org.eclipse.wst.server.launchbar.remote.ModuleObjectProvider"
            id="org.eclipse.wst.server.launchbar.remote.ModuleObjectProvider">
      </objectProvider>
      <descriptorType
            class="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            id="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            priority="5">
      </descriptorType>
      <configType
            descriptorType="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            isDefault="false"
            launchConfigurationType="org.eclipse.wst.server.launchbar.remote.serverAdapterLaunch"
            targetType="org.eclipse.wst.server.launchbar.remote.connection">
      </configType>
      <configProvider
            descriptorType="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            class="org.eclipse.wst.server.launchbar.remote.ModuleLaunchConfigurationProvider"
            launchConfigurationType="org.eclipse.wst.server.launchbar.remote.serverAdapterLaunch"
            priority="1">
      </configProvider>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.eclipse.wst.server.launchbar.remote.ServerLaunchBarDelegate"
            delegateDescription="A launch for executing the &quot;Run on Server&quot; action"
            delegateName="Run On Server Launch"
            id="org.eclipse.wst.server.launchbar.remote.serverAdapterLaunch"
            modes="run,debug,profile"
            name="Server Adapter Launchbar Configuration"
            public="false">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.launchbar.ui.launchBarUIContributions">
      <descriptorUI
            descriptorTypeId="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            labelProvider="org.eclipse.wst.server.launchbar.remote.ui.ModuleDescriptorLabelProvider">
      </descriptorUI>
      <descriptorUI
            descriptorTypeId="org.eclipse.wst.server.launchbar.remote.ModuleDescriptorType"
            labelProvider="org.eclipse.wst.server.launchbar.remote.ui.ModuleDescriptorLabelProvider">
      </descriptorUI>
   </extension>

</plugin>
